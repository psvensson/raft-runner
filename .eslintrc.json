{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "printWidth": 150,
    "semi": 1,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "none",
    "endOfLine": "auto"
    }
}
